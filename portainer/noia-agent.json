[
{
    "type": "1",
    "title": "NOIA agent",
    "description": "Connecting machines",
    "categories": ["Networking", "VPN"],
    "platform": "linux",
    "logo": "https://portainer-io-assets.sfo2.digitaloceanspaces.com/logos/sematext_agent.png",
    "image": "noia/agent:latest",
    "name": "noia-agent",
    "network": "host",
    "env": [
      {
        "name": "NOIA_API_KEY",
        "label": "xx"
      },
      {

        "name": "NOIA_CONTROLLER_URL",
        "label": "app-controller-platform-agents.noia.network"
      },
      {
        "name": "NOIA_NETWORK_API",
        "label": "docker"

      },
      {
        "name": "NOIA_TAGS",
        "label": "x"

      },

      {
        "name": "NOIA_AGENT_NAME",
        "label": "x"
      },
      {
        "name": "NOIA_COUNTRY",
        "label": "x"

      },
      {

        "name": "NOIA_CITY",
        "label": "x"
      },
      {
        "name": "NOIA_CATEGORY",
        "label": "x"

      },
      {

        "name": "NOIA_PROVIDER",
        "label": "x"
      },
      {
        "name": "NOIA_LAT",
        "label": "x"

      },
      {

        "name": "NOIA_LON",
        "label": "x"
      },
      {
        "name": "NOIA_TAGS",
        "label": "x"
      }
    ],
    "volumes": [
        {
            "container": "/var/run/docker.sock",
            "bind": "/var/run/docker.sock",
            "readonly": true
        }
    ]
  }
]

